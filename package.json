{
  "name": "esm-middleware",
  "version": "1.0.4",
  "description": "Serve ES modules from your node_modules folder.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fpipita/esm-middleware.git"
  },
  "homepage": "https://github.com/fpipita/esm-middleware/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/fpipita/esm-middleware/issues"
  },
  "author": "Francesco Pipita <francesco.pipita@gmail.com>",
  "license": "GPL-3.0",
  "private": false,
  "keywords": [
    "esm",
    "es6",
    "modules",
    "express",
    "server",
    "middleware",
    "resolve",
    "transform"
  ],
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/template": "^7.4.4",
    "@babel/types": "^7.4.0",
    "mime-types": "^2.1.22"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "express": "^4.17.1",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "standard-changelog": "^2.0.11",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "test": "eslint . && jest",
    "debug": "node --inspect-brk node_modules/.bin/jest --no-cache --runInBand",
    "preversion": "yarn test",
    "version": "standard-changelog && git add -A",
    "postversion": "git push && git push --tags"
  },
  "lint-staged": {
    "*.js": [
      "prettier",
      "eslint",
      "git add"
    ],
    "*.md": [
      "prettier",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
